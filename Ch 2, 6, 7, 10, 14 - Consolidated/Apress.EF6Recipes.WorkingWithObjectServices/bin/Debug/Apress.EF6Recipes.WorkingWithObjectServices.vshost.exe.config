  <connectionStrings>
    <add name="EF6CodeFirstRecipesContext" connectionString="data source=.\SQLEXPRESS;initial catalog=EF6Recipes;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
